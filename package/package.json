{
    "name": "react-redux-test-renderer",
    "version": "3.0.0",
    "description": "Simplifies testing with testing-library and redux",
    "author": "ted shaughnessy",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/TedShaughnessy/react-redux-test-renderer"
    },
    "keywords": [
        "react",
        "redux",
        "react-redux",
        "testing-library",
