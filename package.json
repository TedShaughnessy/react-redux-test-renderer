{
    "name": "int-tests",
    "version": "0.0.1",
    "description": "integration tests for the react-redux-test-renderer package",
    "author": "ted shaughnessy",
    "license": "MIT",
    "repository": {
        "type": "local",
        "url": ""
    },
    "scripts": {
        "test": "jest -all --coverage",
        "build": "cd package && npm run build",
        "lint": "eslint --ext=ts,tsx package/src --fix",
